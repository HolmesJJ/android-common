<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
    <declare-styleable name="FixedAspectRatioRelativeLayout">
        <attr name="aspect_ratio_width" format="integer" />
        <attr name="aspect_ratio_height" format="integer" />
